#ifndef __CONTEXT
#error __FILE__ : __CONTEXT not defined
#endif
#ifndef __COMM
#error __FILE__ : __COMM not defined
#endif
#define __SNAME __CONCAT(__CONTEXT, _EXEC)
!=================================================================================
       subroutine  __SNAME ()  ! execute command block
!
       __DEP_PARSER
#ifdef __PARALLEL
       __DEP_MPI
       __DEP_MULTICOM
#endif
!
       __IAM(__SNAME)
       character(len=maxlinelen), pointer :: command
       int :: l
#ifdef __MUSTINIT
       if (.not. __CONCAT(__CONTEXT,_initialized)) call __CONCAT(__CONTEXT,_init) ()
#endif
       if (noexec(__STRING(__CONTEXT))) return
__NULL_COMM_STOP
       call message(whoami,'Executing commands within context "'//&
&                                                                  trim(__STRING(__CONTEXT))//'"')
!
       do
        command=>get_next_command(__STRING(__CONTEXT))
        if (.not.associated(command)) exit
#ifdef __DEBUG
        call message(whoami,' CURRENT COMMAND IS: "'//trim(command)//&
        &'".',__DBGLEV)
#endif
        call toupper(command)
        l=len_trim(command)
!
        call message(whoami, ' =====> "'//command(1:l)//'"');
!
        select case(command(1:l))
! body
#define __CASE(_IF,_DO) case(_IF) ; call __CONCAT3(__CONTEXT,_,_DO) () ;
#if (defined (_IFS1) && defined _DOS1)
        __CASE(_IFS1, _DOS1)
#undef  _IFS1
#undef  _DOS1
#endif
#if (defined (_IFS2) && defined _DOS2)
        __CASE(_IFS2, _DOS2)
#undef  _IFS2
#undef  _DOS2
#endif
#if (defined (_IFS3) && defined _DOS3)
        __CASE(_IFS3, _DOS3)
#undef  _IFS3
#undef  _DOS3
#endif
#if (defined (_IFS4) && defined _DOS4)
        __CASE(_IFS4, _DOS4)
#undef  _IFS4
#undef  _DOS4
#endif
        case default;
#ifdef __DEFAULTDO
        __DEFAULTDO
#undef __DEFAULTDO
#else
        call warning(whoami, 'Unknown command. Skipping.',-1)
#endif
        end select
!
        if (fatal_warning(__COMM)) return
!
        deallocate(command)
       enddo
#ifdef __DEBUG
        call message(whoami,' FINISHED EXECUTING STACK'//&
        &'".',__DBGLEV)
#endif
!
       end subroutine __SNAME
!=================================================================================
