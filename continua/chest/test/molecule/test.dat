# test case for computing grid densities based on a molecule
# grid size
continuum {
 nx=98
 ny=98
 nz=98

 xgrid=object
 ygrid=object
 zgrid=object

! xstretchfix=no ! defaults to yes
 xoptimize=no
 yoptimize=no
 zoptimize=no

}

object {
 object=molecule
}

molecule{
 structure=traf-rank.psf
 coordinates=traf-rank.pdb
 coortype=pdb
}

ches{
 output_format=plot3d
 exec:init
}


