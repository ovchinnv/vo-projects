
# size

nx=66; ny=66; nz=66

# grid

xgrid=exp; xstretch=1.05; x0=0; x1=1
ygrid=exp; ystretch=1.05; y0=0; y1=2
zgrid=exp; zstretch=1.05; z0=0; z1=1.5

# BC

bcx0=dirichlet; bcx0file=xbc0_test.dat
bcx1=dirichlet; bcx1file=xbc1_test.dat
bcy0=dirichlet; bcy0file=ybc0_test.dat
bcy1=dirichlet; bcy1file=ybc1_test.dat
bcz0=dirichlet; bcz0file=zbc0_test.dat
bcz1=dirichlet; bcz1file=zbc1_test.dat

# initial 3D data

epsinit=constant   ; epsconst=1.
kappainit=constant ; kappaconst=0.
chargeinit=file    ; chargefile=rhs_test.dat ;  charge_mode=text ; charge_format=chest
phiinit=constant   ; phiconst=0. % initial solution
# multigrid solver parameters

solver=multigrid
multigrid_maximum_cycles=25
multigrid_smooth_iterations=7
multigrid_maximum_levels=20
multigrid_smoother=Jacobi
#multigrid_smoother=Gauss-Seidel
#multigrid_smoother=Jacobi-Tiled-LM
#multigrid_compute_ini_residual=no
#multigrid_skip_convergence_test=14
#multigrid_smoother_unroll=-1
multigrid_omega=0.93
#

output_format=plot3d
output_mode=text
gridoutput=xyz_test.xyz
potoutput=solution.dat


