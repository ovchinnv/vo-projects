       implicit none					      |	       __IMPNONE
       implicit none					      |	       __IMPNONE
       implicit none					      |	       __IMPNONE
       character*8, parameter :: whoami = 'PSF_READ'	      |	       character(len=8, parameter :: whoami = 'PSF_READ'
       character*200 :: keyword, cmdline		      |	       character(len=200 :: keyword, cmdline
       character*8 :: segid, resid, resname, type, aname      |	       character(len=8 :: segid, resid, resname, type, aname
       implicit none					      |	       __IMPNONE
